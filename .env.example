# Citadel Agent Environment Variables

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=citadel_agent
DB_SSL_MODE=disable

# Redis Configuration
REDIS_ADDRESS=localhost:6379
REDIS_PASSWORD=

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-here-make-it-long-and-random

# OAuth Configuration
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_REDIRECT_URI=http://localhost:5001/auth/github/callback

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:5001/auth/google/callback

# Admin User Configuration
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
ADMIN_EMAIL=admin@citadel-agent.com
ADMIN_FIRST_NAME=Admin
ADMIN_LAST_NAME=User

# Default OAuth User Configuration
GITHUB_DEFAULT_USERNAME=github_user
GITHUB_DEFAULT_EMAIL=github_user@example.com
GOOGLE_DEFAULT_USERNAME=google_user
GOOGLE_DEFAULT_EMAIL=google_user@example.com

# Test Configuration (for test files)
TEST_USER_EMAIL=test@example.com
TEST_USER_USERNAME=testuser
TEST_USER_PASSWORD=password123